
#include <iostream>
using namespace std;

int main() {
double num1, num2;

cout << "enter first number" <<endl;
cin >> num1;
cout << "enter second number" <<endl;
cin >> num2;
 
 cout << "\nresults:\n";
 cout << " sum = " << num1 + num2 << endl;
 cout << " difference = " << num1 - num2 << endl;
 cout << " product = " << num1 * num2 << endl;
 cout << " quotient = " << num1 / num2 << endl;
 
  
  
  
  
  
  
  
  
  return 0;
}
